<h1>Admin</h1>


<h2>Create New Customer</h2>
<div id="clearance" class="sign-up">

  <h2><%#= t(".title") %></h2>

  <%= form_for @newUser do |form| %>
      <%= render partial: '/users/form', object: form %>

      <div class="submit-field">
        <%= form.submit %>
      </div>

      <div class="other-links">
        <%= link_to t(".sign_in"), sign_up_url %>
      </div>
  <% end %>
</div>


<h2>Customers</h2>
