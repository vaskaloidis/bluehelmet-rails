<p id="notice"><%= notice %></p>

<h1>User Contents</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Content</th>
      <th>Sticky</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_contents.each do |user_content| %>
      <tr>
        <td><%= user_content.user_id %></td>
        <td><%= user_content.content_id %></td>
        <td><%= user_content.sticky %></td>
        <td><%= link_to 'Show', user_content %></td>
        <td><%= link_to 'Edit', edit_user_content_path(user_content) %></td>
        <td><%= link_to 'Destroy', user_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Content', new_user_content_path %>
