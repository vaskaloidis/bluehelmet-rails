<%= simple_form_for(@template) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :purchaseUrl %>
    <%= f.input :demoUrl %>
    <%= f.association :TemplateType %>
    <%= f.association :TemplateCategory %>
    <%= f.input :price %>
    <%= f.input :tags %>
    <%= f.input :compatibleBrowsers %>
    <%= f.input :bootstrapVersions %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
